# LLM model for top-level calls (default: claude-opus-4-5)
# RLM_MODEL=claude-opus-4-5

# LLM model for recursive child calls (default: same as RLM_MODEL)
# RLM_CHILD_MODEL=claude-sonnet-4-5-20250929

# Max explore-mode steps per run (default: 20)
# RLM_MAX_EXPLORE_STEPS=20

# Max commit-mode cycles per run (default: 5)
# RLM_MAX_COMMIT_CYCLES=5

# Max recursion depth for rlm_call/map (default: 1)
# RLM_MAX_RECURSION_DEPTH=1

# Max parallel jobs for map operations (default: 4)
# RLM_MAX_PARALLEL_JOBS=4

# Cache directory for content-addressed results (default: ~/.cache/rlm-nix-wasm)
# RLM_CACHE_DIR=~/.cache/rlm-nix-wasm

# Enable Nix sandboxing (default: false)
# RLM_USE_NIX=false

# Verbose output (default: false)
# RLM_VERBOSE=false

# Path to CPython WASI binary for eval sandboxing (required for eval ops)
# RLM_WASM_PYTHON_PATH=/path/to/python.wasm

# CPU fuel limit for WASM sandbox (default: 10000000000)
# RLM_WASM_FUEL=10000000000

# Memory limit in MB for WASM sandbox (default: 256)
# RLM_WASM_MEMORY_MB=256
